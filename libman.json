{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.4.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "vue@2.6.10",
      "destination": "wwwroot/lib/vue"
    },
    {
      "library": "axios@0.19.0",
      "destination": "wwwroot/lib/axios"
    },
    {
      "library": "font-awesome@5.9.0",
      "destination": "wwwroot/lib/font-awesome"
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper"
    },
    {
      "library": "sweetalert@1.1.3",
      "destination": "wwwroot/lib//sweetalert"
    },
    {
      "provider": "jsdelivr",
      "library": "@ckeditor/ckeditor5-build-classic@12.3.1",
      "destination": "wwwroot/lib/ckeditor5-build-classic"
    },
    {
      "provider": "jsdelivr",
      "library": "@ckeditor/ckeditor5-vue@1.0.0-beta.2",
      "destination": "wwwroot/lib/ckeditor5-vue"
    }
  ]
}
